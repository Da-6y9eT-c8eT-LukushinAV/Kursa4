AFTER UPDATE ON Bookings
FOR EACH ROW
BEGIN
    UPDATE Bookings
    SET TotalPrice = (JULIANDAY(NEW.CheckOutDate) - JULIANDAY(NEW.CheckInDate)) * 
                     (SELECT PricePerNight FROM Rooms WHERE RoomId = NEW.RoomId)
    WHERE BookingId = NEW.BookingId;
END;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
